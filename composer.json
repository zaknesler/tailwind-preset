{
    "name": "zaknesler/tailwind-preset",
    "description": "A Tailwind CSS preset for Laravel.",
    "keywords": ["laravel", "preset", "tailwind", "tailwindcss"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zak Nesler",
            "email": "zak@nesler.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2",
        "laravel/ui": "^2.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "ZakNesler\\TailwindPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["ZakNesler\\TailwindPreset\\TailwindServiceProvider"]
        }
    }
}
